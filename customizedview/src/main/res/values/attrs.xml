<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PathView">
        <attr name="direction" format="boolean"/>
        <attr name="fillType" format="enum">
            <enum name="winding" value="0"/>
            <enum name="even-odd" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ShaderView">
        <attr name="tile_mode" format="enum">
            <enum name="clamp" value="CLAMP"/>
            <enum name="repeat" value="REPEAT"/>
            <enum name="mirror" value="MIRROR"/>
        </attr>
    </declare-styleable>

</resources>